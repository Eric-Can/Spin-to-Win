ipts.has(script);\n  } else {\n    // Fallback to inferring from language\n    var lang = locale.split('-')[0];\n    isRTL = rtlLangs.has(lang);\n  }\n\n  cache.set(locale, isRTL);\n  return isRTL;\n}"},"lineCount":null}},"error":null,"hash":"47b98fec738d09950372f372c053e93a","cacheData":{"env":{}}}